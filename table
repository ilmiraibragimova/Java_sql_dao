CREATE TABLE podraz (
                        idTabl SERIAL PRIMARY KEY,
                        numPodraz INTEGER,
                        namePodraz VARCHAR(15),
                        numOtdel INTEGER,
                        nameOtdel VARCHAR(15),
                        idEmp INTEGER references mytest(id) on delete cascade
);
