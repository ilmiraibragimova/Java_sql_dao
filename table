CREATE TABLE mytest (
    id SERIAL PRIMARY KEY,
    family VARCHAR(20),
    name VARCHAR(10),
    ochestvo VARCHAR(15),
    tel VARCHAR (15),
    adres VARCHAR(50),
    dopInfo VARCHAR(50)
);
